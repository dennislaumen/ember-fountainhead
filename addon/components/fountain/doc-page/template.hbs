{{fountain.doc-page.header
  name=content.name
  file=content.file
  classUrl=(concat content.file '#' content.line)}}

{{fountain.doc-page.attributes
  name=content.name
  extends=content.extends
  file=content.file
  line=content.line
  is_constructor=content.is_constructor}}

{{fountain.doc-page.live-description descriptionText=content.description}}

{{#each content.classitems as |classItem|}}
  {{fountain.doc-page.class-item item=classItem}}
{{/each}}
