{
  "name": "ember-fountainhead",
  "version": "2.0.0-beta.5",
  "description": "Robust interactive documentation for your Ember application's source code",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "prepublish": "npm run sass",
    "build": "ember build",
    "start": "ember server",
    "test": "ember try:each",
    "test:lib": "mocha --recursive lib/*.spec.js lib/**/*.spec.js",
    "sass": "node-sass app/styles/ember-fountainhead.scss vendor/ember-fountainhead.css",
    "sass:watch": "node-sass -w=app/styles app/styles/ember-fountainhead.scss vendor/ember-fountainhead.css",
    "svgs": "svg-sprite --defs --defs-dest=public/svg --defs-sprite=symbol-defs.svg public/svg/svgs/*.svg"
  },
  "repository": "https://github.com/healthsparq/ember-fountainhead",
  "engines": {
    "node": ">= 4.3.2"
  },
  "contributors": [
    "Evan Rowe <ev.rowe@gmail.com>",
    "Dan Hedgecock <hedgecock.d@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.2",
    "broccoli-funnel": "^1.0.1",
    "broccoli-merge-trees": "^1.1.1",
    "ember-ajax": "^2.0.1",
    "ember-cli": "2.8.0",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-dependency-checker": "^1.2.0",
    "ember-cli-eslint": "^3.0.0",
    "ember-cli-inject-live-reload": "^1.4.0",
    "ember-cli-qunit": "^2.1.0",
    "ember-cli-release": "^0.2.9",
    "ember-cli-sass": "^5.5.2",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^1.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-component-css": "^0.2.4",
    "ember-data": "^2.8.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^1.0.5",
    "ember-load-initializers": "^0.5.1",
    "ember-resolver": "^2.0.3",
    "loader.js": "^4.0.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.0.0",
    "svg-sprite": "^1.3.6"
  },
  "keywords": [
    "ember-addon",
    "documentation",
    "yuidoc"
  ],
  "dependencies": {
    "ember-cli-babel": "^5.1.6",
    "ember-cli-htmlbars": "^1.0.3",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-radical": "git://github.com/healthsparq/ember-radical.git#03ed6b1128f703df4711f6c2b90a04ac8bd87ab5",
    "markdown-it": "^8.0.0",
    "prismjs": "^1.5.1",
    "yuidocjs": "^0.10.2"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
